<div class="row">   
    <h1>Add a new Inventory</h1>
    <div class="col-sm-5">
        <form method="post" action="/addItem">
            catagory: <input type="text" name="catagory"><br><br>
            name: <input  type="text" name="name"><br><br>
            price: <input name="price"><br><br>
            size: <input type="text" name="size"><br><br>
            picture: <input  type="file" name="picture"><br><br>
            inventory: <input  type="number" name="inventory"><br><br>
            details: <input  type="text" name="details"><br><br>
            ingredients: <input  type="text" name="ingredients"><br><br>
            warnings: <input  type="text" name="warnings"><br><br>
            directions: <input  type="text" name="directions"><br><br>
            <input type="submit">
        </form>
    </div>
</div>
